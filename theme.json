{
	"name": "Halloween Theme",
	"colors": {
		"editor.background": "#0D0D0D", 
		"editor.foreground": "#FF7518", 
		"activityBarBadge.background": "#FF4500", 
		"sideBarTitle.foreground": "#CCCCCC", 
		"editor.lineHighlightBackground": "#292929", 
		"statusBar.background": "#1b1b1b", 
		"statusBar.foreground": "#FF7518" 
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#666666" 
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#FFDD00" 
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#FF7518" 
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF0000", 
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA" 
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#F78C6C" 
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#FF4500" 
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#82AAFF" 
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#FF7518" 
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF7518"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant"
			],
			"settings": {
				"foreground": "#F78C6C" 
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#C3E88D" 
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type"
			],
			"settings": {
				"foreground": "#FFCB6B" 
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6" 
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6" 
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js"
			],
			"settings": {
				"foreground": "#FF5370" 
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF" 
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#C792EA" 
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF" 
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		}
	]
}
